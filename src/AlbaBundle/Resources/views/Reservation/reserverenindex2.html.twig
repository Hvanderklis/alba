{% extends 'AlbaBundle::base.html.twig' %}

{% block stylesheets %}
    <style>
        .p{
            color: black;
        }
    </style>
{% endblock %}
{% block body %}


    {% set reserveren = app.session.get('reserveren') %}


            <table>
                <thead>
                <tr>
                    <th colspan="4">Reservation</th>
                </tr>
                {% for key ,reserveren in reserveren %}
                <tr>

                    <th>{{ key }}</th>

                </tr>
                </thead>

                <tbody>
                <tr>
                    <td>{{ reserveren }}</td>
                </tr>
                </tbody>
                {% endfor %}
                <tr>
                <td><a href="{{ path('kamer_reserveren') }}" class="button"></a></td>
                </tr>
            </table>
{% endblock %}



